{
  "expo": {
    "name": "ReactNativeSimpleLogin",
    "slug": "ReactNativeSimpleLogin",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/logo.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/logo.png",
      "resizeMode": "fit",
      "backgroundColor": "#3B49DF"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/logo.png",
        "width": "100px",
        "height": "100px",
        "backgroundColor": "#3B49DF"
      }
    },
    "web": {
      "favicon": "./assets/logo.png"
    }
  }
}
